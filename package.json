{
  "name": "cashheros",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.8.3",
    "axios-retry": "^3.8.0",
    "compression": "^1.7.4",
    "express-validator": "^7.2.1",
    "i18next": "^25.0.0",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "jwt-decode": "^4.0.0",
    "localforage": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.5.3",
    "react-lazy-load-image-component": "^1.6.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^7.0.0",
    "workbox-broadcast-update": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-google-analytics": "^7.0.0",
    "workbox-navigation-preload": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-range-requests": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-streams": "^7.0.0"
  },
  "scripts": {
    "start": "react-scripts start --openssl-legacy-provider",
    "start:proxy": "node dev-server.js",
    "start:dev": "start-dev.bat",
    "start:css-fix": "start-with-css-fix.bat",
    "start:simple": "run-simple-server.bat",
    "build": "react-scripts build && npm run generate-sw",
    "build:analyze": "react-scripts build && npm run generate-sw && npx webpack-bundle-analyzer build/bundle-stats.json",
    "postbuild": "echo 'Build completed successfully'",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:a11y": "react-scripts test --testMatch='**/*A11y.test.{js,jsx}'",
    "test:e2e": "playwright test",
    "eject": "react-scripts eject",
    "serve": "node server.js",
    "serve:css-fix": "node css-fix-server.js",
    "serve:simple": "node simple-server.js",
    "dev:proxy": "node dev-server.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "generate-sw": "workbox injectManifest workbox-config.js",
    "optimize-images": "node scripts/optimize-images.js",
    "deploy-fix": "node scripts/deploy-fix.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "bundle-analyzer": "^0.0.6",
    "critters": "^0.0.20",
    "glob": "^10.3.10",
    "http-proxy-middleware": "^3.0.5",
    "imagemin-webp-webpack-plugin": "^3.3.6",
    "jest-axe": "^7.0.1",
    "msw": "^1.2.1",
    "sharp": "^0.33.2",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "workbox-cli": "^7.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/setupTests.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 70,
        "branches": 70,
        "functions": 70,
        "lines": 70
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
